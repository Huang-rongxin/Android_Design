<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zyf.dict.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="25" />
    <!-- 查看网络状态 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 允许访问网络 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        name="zyf.dict.activity.MyApplication"
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:icon="@drawable/cibaicon"
        android:label="@string/app_name" >
        <activity
            android:name="zyf.dict.activity.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="zyf.dict.activity.LoginActivity" />
        <activity android:name="zyf.dict.activity.RegisterActivity" />
        <activity android:name="zyf.dict.activity.MainActivity" />
        <activity android:name="zyf.dict.activity.NotebookActivity" />
        <activity android:name="zyf.dict.activity.ShowItemwordActivity" />
        <activity android:name="zyf.dict.activity.TranslateActivity" />
        <activity android:name="zyf.dict.activity.SelectActivity" />
        <activity android:name="zyf.dict.activity.LishiActivity" />
    </application>

</manifest>